{
  "name": "greyscript",
  "displayName": "GreyScript",
  "description": "Syntax highlighting support for GreyScript from GreyHack.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Programming Languages",
    "Themes"
  ],
  "contributes": {
    "languages": [
      {
        "id": "greyscript",
        "aliases": [
          "GreyScript",
          "greyscript"
        ],
        "extensions": [
          ".gs"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "greyscript",
        "scopeName": "source.gs",
        "path": "./syntaxes/greyscript.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "GreyHack Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/greyhack-theme.json"
      }
    ]
  }
}
